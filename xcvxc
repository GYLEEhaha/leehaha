git branch -M main
git push -u origin mainCONDITION1:=(HHV(HIGH,4)-LLV(LOW,5))/REF(CLOSE,2)<0.03 AND (HIGH-OPEN)<(CLOSE-OPEN)*0.5;

CONDITION2:=(CLOSE-REF(CLOSE,1))/REF(CLOSE,1)>0 AND (REF(CLOSE,1)-REF(CLOSE,5))/REF(CLOSE,2)<0;

CONDITION3:=(LOW>REF(LOW,2) AND OPEN>CLOSE) AND (LOW<REF(LOW,2));

SELECTED:=(CONDITION1 AND CONDITION2 AND CONDITION3);

SELECT IF(SELECTED, CODE, "");
